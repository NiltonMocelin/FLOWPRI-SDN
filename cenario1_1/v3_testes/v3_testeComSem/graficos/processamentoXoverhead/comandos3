library(ggplot2)
dados <-read.table("barras.txt", sep=",", head=TRUE)
dados
tempos = c("2s", "4s", "6s", "8s", "10s", "12s")
grafico = ggplot(dados, aes(fill=funcoes, y=valores, x=cenario)) + 
    geom_bar(position="stack", stat="identity")
grafico +  geom_line(data = percebido, aes(x=cenario, y=Tempo.percebido,group=funcoes, color=funcoes), inherit.aes=FALSE) + geom_point(data=percebido, aes(x=cenario,y=Tempo.percebido,color=funcoes), inherit.aes=FALSE) + scale_color_manual(values=c("black", "#9999CC"))
percebido <- read.table("percebido.txt", head=TRUE, sep=",")
grafico +  geom_line(data = percebido, aes(x=cenario, y=Tempo.percebido,group=funcoes, color=funcoes), inherit.aes=FALSE) + geom_point(data=percebido, aes(x=cenario,y=Tempo.percebido,color=funcoes), inherit.aes=FALSE) + scale_color_manual(values=c("black", "#9999CC"))
dados
grafico = ggplot(dados, aes(fill=funcoes, y=valores, x=cenario)) + 
    geom_bar(position="stack", stat="identity")
grafico
percebido
grafico +  geom_line(data = percebido, aes(x=cenario, y=Tempo.percebido,group=total, color=total), inherit.aes=FALSE) + geom_point(data=percebido, aes(x=cenario,y=Tempo.percebido,color=total), inherit.aes=FALSE) + scale_color_manual(values=c("black", "#9999CC")) +  theme_bw() + theme(text = element_text(size = 20))
